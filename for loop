order:

import java.util.Scanner;
class main {

    public static void main(String[] args) {

        for(int i=0;i<=10;i++){
        
        
            System.out.println(i);
        
        }
    }
    }


reverse order:
import java.util.Scanner;
class main {

    public static void main(String[] args) {

        for(int i=10;i>=0;i--){
        
        
            System.out.println(i);
        
        }
    }
    }
